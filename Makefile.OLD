RM = rm -f
ECHO = echo
DIR = src
SUBDIRS = src
bin_PROGRAMS = lcs lcc lct lcg lcu ssha lcr lcdb

all:	$(bin_PROGRAMS)

lcs:
	cd $(DIR); make $(MFLAGS) $@

lcc:
	cd $(DIR); make $(MFLAGS) $@

lct:
	cd $(DIR); make $(MFLAGS) $@

lcg:
	cd $(DIR); make $(MFLAGS) $@

lcu:
	cd $(DIR); make $(MFLAGS) $@

lcr:
	cd $(DIR); make $(MFLAGS) $@

lcdb:
	cd $(DIR); make $(MFLAGS) $@

ssha:
	cd $(DIR); make $(MFLAGS) $@

clean:
	cd $(DIR); make $@